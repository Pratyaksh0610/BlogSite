<%-include('partials/header');-%>
<h1>Compose</h1>

<form action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input type="text" class="form-control" name="inpTitle">
    </div>
    <div class="form-group">
      <label>Post</label>
      <textarea class="form-control" rows="3" name="inpContent"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%-include('partials/footer');-%>